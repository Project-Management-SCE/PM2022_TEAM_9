<StackPane prefHeight="650" prefWidth="650" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.registerController">
    <children>
        <StackPane>
            <children>

                <VBox spacing="10.0">
                    <children>
                        <GridPane prefHeight="641.0" prefWidth="655.0">
                            <children>
                                <Label text="Username:" GridPane.rowIndex="2">
                                    <font>
                                        <Font size="17.0" />
                                    </font></Label>
                                <Label text="Password:" GridPane.rowIndex="4">
                                    <font>
                                        <Font size="17.0" />
                                    </font></Label>
                                <TextField fx:id="username" prefHeight="35.0" prefWidth="362.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                                <PasswordField fx:id="pass" prefHeight="33.0" prefWidth="362.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
