<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane prefHeight="650" prefWidth="650" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.LoginController">
    <children>
        <StackPane>
            <children>

                <VBox spacing="10.0">
                    <children>
                        <GridPane prefHeight="641.0" prefWidth="655.0">
                            <children>
                                <Label text="Username:" GridPane.rowIndex="2" />
                                <Label text="Password:" GridPane.rowIndex="3" />
                                <TextField fx:id="user" promptText="Use &quot;tamaram&quot; to login" text="tamaram" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                                <PasswordField fx:id="password" prefHeight="26.0" prefWidth="199.0" promptText="Use &quot;PM2022&quot; to login" text="PM2022" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                                <Button fx:id="loginButton" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="Login" GridPane.columnIndex="2" GridPane.rowIndex="4" />
                      <Button fx:id="back" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="back" />
                            </children>
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="77.6" />
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="295.0" minWidth="10.0" prefWidth="286.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0" prefWidth="170.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                        </GridPane>
                    </children>
                </VBox>
            </children>
        </StackPane>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</StackPane>
